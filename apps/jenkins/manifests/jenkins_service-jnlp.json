{
   "apiVersion": "v1",
   "kind": "Service",
   "metadata": {
      "name": "jenkins-jnlp",
      "namespace": "jenkins"
   },
   "spec": {
      "ports": [
         {
            "nodePort": 32000,
            "port": 50000,
            "targetPort": "jnlp"
         }
      ],
      "selector": {
         "app": "jenkins"
      },
      "type": "NodePort"
   }
}
