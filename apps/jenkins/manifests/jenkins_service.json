{
   "apiVersion": "v1",
   "kind": "Service",
   "metadata": {
      "annotations": {
         "prometheus.io/path": "/",
         "prometheus.io/port": "8080",
         "prometheus.io/scrape": "true"
      },
      "name": "jenkins",
      "namespace": "jenkins"
   },
   "spec": {
      "ports": [
         {
            "name": "http",
            "port": 80,
            "targetPort": "http"
         }
      ],
      "selector": {
         "app": "jenkins"
      }
   }
}
